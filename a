  axios(config1)
      .then(function (response) {
        setHasError(false)
        codigo = JSON.stringify(response.data.result.upload_id)
        codigo = codigo.replace(/['"]+/g, '')
        console.log(codigo)
        setUploadId(codigo)
        var config = {
          headers: {
            'apiKey': APIKEY,
            'apiSecret': APISECRET,
            'Authorization': 'Basic ' + AUTHORIZATION,
            'Accept': 'application/json',
            'Content-Type': 'application/json'
          }
        };
        console.log('upload code: ' + response.data.result.upload_id)
        var url = URL+'?image_upload_id=' + response.data.result.upload_id
      console.log(url)
      setMessageStatus('Analyzing picture ...')
        axios.get(url, config)
          .then(function (response) {
            setResultTagging(response.data)
            setHasError(false)
            console.log(JSON.stringify(response));
           setMessageStatus(null)
          })
          .catch(function (error) {
            setHasError(true);
            errorText(JSON.stringify(error.status.text))
            errorType(JSON.stringify(error.status.type))
            console.log(error);
          });

      })
      .catch(function (error) {
        setHasError(true);
            errorText(JSON.stringify(error.status.text))
            errorType(JSON.stringify(error.status.type))
        console.log(error);
      });
  }